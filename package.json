{
  "name": "preact-vite-starter",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-esm server/server.ts",
    "build:client": "NODE_ENV=production PLATFORM=browser vite build --outDir dist/client",
    "build:server": "NODE_ENV=production PLATFORM=server vite build --ssr src/entryServer.tsx --outDir dist/server",
    "serve": "NODE_ENV=production ts-node-esm server/server.ts",
    "build:serve": "pnpm build:client && pnpm build:server && pnpm serve"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "million": "^2.6.0",
    "preact": "^10.17.1",
    "preact-render-to-string": "^6.2.1",
    "preact-ssr-prepass": "^1.2.0",
    "serve-static": "^1.15.0",
    "wouter-preact": "^2.11.0"
  },
  "devDependencies": {
    "@prefresh/vite": "^2.4.1",
    "@rollup/pluginutils": "^5.0.4",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.18",
    "@types/serve-static": "^1.15.3",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "debug": "^4.3.4",
    "kolorist": "^1.8.0",
    "prettier": "^3.0.3",
    "terser": "^5.20.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
